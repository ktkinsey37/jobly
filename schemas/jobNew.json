{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/example.json",
    "type": "object",
    "properties": {
      "title": {
        "type": "string",
        "minLength": 1,
        "maxLength": 45
      },
      "salary": {
        "type": "integer",
        "minimum": 0
      },
      "equity": {
        "type": "number",
        "maximum": 1.0
      },
      "company_handle": {
        "type": "string",
        "minLength": 1,
        "maxLength": 25
      }
    },
    "additionalProperties": false,
    "required": [
      "title",
      "company_handle"
    ]
  }
  